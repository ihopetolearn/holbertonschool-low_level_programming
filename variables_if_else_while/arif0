#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main()
{   int n;
	srand(time(0));
	n=rand() -RAD_MAX/2;
	if (n>0)
	printf("%d is positive\n",n);
	else if (n<0)
	printf("%d is negative\n",n);
	else (n=0)
	printf("%d equal to zero\n",n);
	return (0) 


}

